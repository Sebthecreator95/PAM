{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/eventSchema.json",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string",
      "minLength": 1
    },
    "eventId": {
      "type": "string",
      "minLength": 1
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "time": {
      "type": "string"
    },
    "date": {
      "type": "string"
      
    },
    "Icon": {
      "type": "string"
    }
  },
  "additionalProperties": true,
  "required": [
    "userId",
    "eventId",
    "name",
    "date"
  ]
}
